From 350d9d9cbc84ee7f4c2678685bb3d44d4f191c94 Mon Sep 17 00:00:00 2001
From: Ming Liu <ming.liu@toradex.com>
Date: Tue, 4 Feb 2020 13:40:42 +0100
Subject: [PATCH] requirements.txt/setup.py: drop tls and ssh dependencies

Upstream-Status: Inappropriate [OE specific configuration]

Signed-off-by: Ming Liu <ming.liu@toradex.com>
---
 requirements.txt | 11 -----------
 setup.py         | 14 --------------
 2 files changed, 25 deletions(-)

diff --git a/requirements.txt b/requirements.txt
index d93cc6f..45a3c96 100644
--- a/requirements.txt
+++ b/requirements.txt
@@ -1,18 +1,7 @@
 appdirs==1.4.3
-asn1crypto==0.22.0
-backports.ssl-match-hostname==3.5.0.1
-cffi==1.10.0
-cryptography==1.9; python_version == '3.3'
-cryptography==2.3; python_version > '3.3'
 docker-pycreds==0.4.0
 enum34==1.1.6
-idna==2.5
 ipaddress==1.0.18
-packaging==16.8
-paramiko==2.4.2
-pycparser==2.17
-pyOpenSSL==18.0.0
-pyparsing==2.2.0
 pypiwin32==219; sys_platform == 'win32' and python_version < '3.6'
 pypiwin32==223; sys_platform == 'win32' and python_version >= '3.6'
 requests==2.20.0
diff --git a/setup.py b/setup.py
index 94fbdf4..54290a4 100644
--- a/setup.py
+++ b/setup.py
@@ -31,20 +31,6 @@ extras_require = {
 
     # urllib3 drops support for Python 3.3 in 1.23
     ':python_version == "3.3"': 'urllib3 < 1.23',
-
-    # If using docker-py over TLS, highly recommend this option is
-    # pip-installed or pinned.
-
-    # TODO: if pip installing both "requests" and "requests[security]", the
-    # extra package from the "security" option are not installed (see
-    # https://github.com/pypa/pip/issues/4391).  Once that's fixed, instead of
-    # installing the extra dependencies, install the following instead:
-    # 'requests[security] >= 2.5.2, != 2.11.0, != 2.12.2'
-    'tls': ['pyOpenSSL>=17.5.0', 'cryptography>=1.3.4', 'idna>=2.0.0'],
-
-    # Only required when connecting using the ssh:// protocol
-    'ssh': ['paramiko>=2.4.2'],
-
 }
 
 version = None
-- 
2.7.4

