 setup.cfg | 2 --
 1 file changed, 2 deletions(-)

diff --git a/setup.cfg b/setup.cfg
index 81b5727..04dd58c 100644
--- a/setup.cfg
+++ b/setup.cfg
@@ -9,9 +9,6 @@ universal = 1
 [metadata]
 license_file = LICENSE.txt
 provides-extra = 
-	secure
-	socks
-	brotli
 requires-dist = 
 	pyOpenSSL>=0.14; extra == 'secure'
 	cryptography>=1.3.4; extra == 'secure'
