if test ${distro_bootpart} != 1
then
    echo "Boot partition needs to be the first partition"
    exit
fi

env set bootcmd_otenv 'load ${devtype} ${devnum}:2 ${scriptaddr} /boot/loader/uEnv.txt; env import -t ${scriptaddr} ${filesize}'
env set bootcmd_load_f 'load ${devtype} ${devnum}:2 ${ramdisk_addr_r} "/boot"${kernel_image}'
env set bootcmd_run '@@KERNEL_BOOTCMD@@ ${ramdisk_addr_r}:kernel@1 ${ramdisk_addr_r}:ramdisk@1 ${fdt_addr}'
env set bootcmd 'run bootcmd_otenv; run bootcmd_load_f; run bootcmd_run'
run bootcmd
